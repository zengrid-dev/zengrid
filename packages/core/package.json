{
  "name": "@zengrid/core",
  "version": "1.1.0",
  "description": "High-performance data grid library with 14 advanced data structures and virtual scrolling",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    },
    "./dist/styles.css": "./dist/src/styles.css",
    "./styles/*": "./dist/src/*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "grid",
    "data-grid",
    "table",
    "spreadsheet",
    "virtual-scroll",
    "datagrid",
    "angular",
    "react",
    "vue"
  ],
  "author": "ZenGrid Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zengrid-dev/zengrid.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/zengrid-dev/zengrid/issues"
  },
  "homepage": "https://github.com/zengrid-dev/zengrid#readme",
  "dependencies": {
    "@preact/signals-core": "^1.13.0",
    "@zengrid/shared": "^1.1.0",
    "vanilla-calendar-pro": "^3.0.5"
  }
}
