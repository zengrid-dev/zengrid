{
  "name": "zengrid",
  "version": "1.0.0",
  "private": true,
  "description": "ZenGrid - High-performance data grid library",
  "scripts": {
    "build": "nx run-many -t build",
    "build:core": "nx build core",
    "build:shared": "nx build shared",
    "test": "nx run-many -t test",
    "test:core": "nx test core",
    "test:shared": "nx test shared",
    "test:watch": "nx test core --watch",
    "test:coverage": "nx test core --coverage",
    "test:perf": "nx test packages/core/test/performance/benchmarks.spec.ts",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:ui": "playwright test --ui",
    "lint": "nx run-many -t lint",
    "docs:api": "typedoc",
    "docs:serve": "cd docs/api && python3 -m http.server 8000",
    "demo": "cd apps/demo && pnpm dev",
    "demo:build": "cd apps/demo && pnpm build",
    "demo:angular": "cd apps/angular-demo && pnpm start",
    "demo:angular:build": "cd apps/angular-demo && pnpm build",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"packages/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,json,md}\""
  },
  "keywords": [
    "grid",
    "data-grid",
    "table",
    "spreadsheet",
    "virtual-scroll"
  ],
  "author": "ZenGrid Team",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/compiler-cli": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@nx/eslint": "^22.5.2",
    "@nx/jest": "^22.3.3",
    "@nx/js": "^22.3.3",
    "@nx/rollup": "^22.3.3",
    "@playwright/test": "^1.48.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ng-packagr": "^21.1.0",
    "nx": "^22.3.3",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  }
}
